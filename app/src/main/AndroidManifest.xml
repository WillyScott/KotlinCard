<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dad.kotlincard">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.example.dad.kotlincard.db.MyApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
       <activity
            android:name=".setcard.SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".setcard.SetCardListActivity">
        </activity>
        <activity android:name=".FlashCard.FlashCardListActivity">
        </activity>
        <activity android:name=".setcard.EditSetCardActivity">
        </activity>
        <activity android:name=".FlashCard.EditFlashCardActivity">
        </activity>
        <activity android:name=".startcards.StartCardsActivity">
        </activity>
        <activity android:name=".setcard.NewSetCardActivity">
        </activity>
        <activity android:name=".FlashCard.NewFlashCardActivity">
        </activity>
        <activity android:name=".export.ExportCardsActivity">
        </activity>

    </application>

</manifest>